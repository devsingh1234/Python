import subprocess
subprocess.call("ifconfig",shell=True)
#subprocess.call("ifcofig wlan0 up",shell=True)
subprocess.call("ifconfig wlan0 down",shell=True)
subprocess.call("ifconfig wlan0 hw ether 14:f4:b6:ad:87:48",shell=True)
subprocess.call("ifcofig wlan0 up",shell=True)
subprocess.call("ifconfig",shell=True)
